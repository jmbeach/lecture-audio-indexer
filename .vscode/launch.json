  "version": "0.2.0",
  "configurations": [
    {
      "name": "Attach",
      "port": 9229,
      "request": "attach",
      "skipFiles": ["<node_internals>/**"],
      "type": "pwa-node"
    },
    {
      "name": "Debug Parse",
      "port": 9229,
      "request": "launch",
      "type": "pwa-node",
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "runtimeArgs": ["--inspect-brk", "./bin/run"],
      "args": ["parse"]
    }
  ]
}
